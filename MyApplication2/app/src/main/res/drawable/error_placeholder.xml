<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="100dp"
    android:height="100dp"
    android:viewportWidth="100"
    android:viewportHeight="100">

    <!-- Фон плейсхолдера -->
    <path
        android:fillColor="#E0E0E0"
        android:pathData="M10,10 L90,10 L90,90 L10,90 Z" />

    <!-- Абстрактное изображение "картинки" (например, рамка с изображением) -->
    <path
        android:fillColor="#B0BEC5"
        android:pathData="M20,20 L80,20 L80,40 L20,40 Z" /> <!-- верхняя часть картинки -->
    <path
        android:fillColor="#B0BEC5"
        android:pathData="M20,40 L80,40 L80,60 L20,60 Z" /> <!-- середина картинки -->
    <path
        android:fillColor="#B0BEC5"
        android:pathData="M20,60 L80,60 L80,80 L20,80 Z" /> <!-- нижняя часть картинки -->

    <!-- Перечеркнутая линия -->
    <path
        android:strokeColor="#9D9D9D"
        android:strokeWidth="5"
        android:pathData="M10,10 L90,90" />

    <path
        android:strokeColor="#9D9D9D"
        android:strokeWidth="5"
        android:pathData="M90,10 L10,90" />
</vector>